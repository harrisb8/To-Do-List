$("#button").click(t=>{let e=$("#input").val();if(""===e)alert("You must write something!");else{$("#input").val("");let t=$("<li></li>");t.text(e),$("#list").append(t),t.on("dblclick",function(){t.toggleClass("strike")});let l=$("<crossOutButton></crossOutButton>");l.append(document.createTextNode("X")),t.append(l),l.on("click",function(t){let e=t.target.parentNode;$(e).remove()}),$("#list").sortable()}});